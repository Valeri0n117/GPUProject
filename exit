[32m+[m[32mvoid verifyDyagonal(float *distanze, int n){[m
[32m+[m
[32m+[m	[32mfor(int i=1; i<n ; i++)[m
[32m+[m		[32mif(distanze[((i*(i-1))/2)+i-1] != 1 )[m
[32m+[m			[32mprintf("%d not one \n", i);[m
[32m+[m
[32m+[m[32m}[m
